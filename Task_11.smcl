sysuse nlsw88
tab race
tab race, nolabel
gen white=1 if race==1
br
br race white
replace white=0 if race==3
br
replace white=0 if race==2
drop white
gen white=1 if race==1
replace white=0 if white==1
br
br race white
gen black =0 if race==2
br
replace black =0 if black==0
tab white
tab black
gen new=age+race
br
egen new1=mean(age)
br age new1
egen new2=max(age)
br age new2
br age white
bys white:egen mean_age=mean age
bys white:egen mean_age=mean(age)
br
br age white mean_age
